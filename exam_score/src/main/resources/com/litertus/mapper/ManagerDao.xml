<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.score.litertus.dao.managerDao">
    <insert id="save" parameterType="Manager" useGeneratedKeys="true" keyProperty="Uid">
        insert into user_detail values (#{Uid},#{account},#{password},#{sex},#{status},#{registerTime},#{permisson})
    </insert>

</mapper>